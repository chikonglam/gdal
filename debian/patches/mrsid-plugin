+	rm -rf libgdal-mrsid-$(GDAL_VER)/.svn
+	cp *.h libgdal-mrsid-$(GDAL_VER)
+	cp *.cpp libgdal-mrsid-$(GDAL_VER)
+	tar czvf libgdal-mrsid-$(GDAL_VER).tar.gz ./libgdal-mrsid-$(GDAL_VER)
+	rm -rf libgdal-mrsid-$(GDAL_VER)
